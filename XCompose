# -*- conf -*-
#
# Simon Tatham's personal list of extra compose-key sequences.
#
# You need to set GTK_IM_MODULE=xim in the environment for this file
# to be honoured at all.

include "%L"

# Floor and ceiling brackets. The _ vs ^ in the first composed key is
# intended to recall TeX's subscript/superscript notation, and then of
# course the following bracket indicates whether it's opening or
# closing.
<Multi_key> <underscore> <parenleft> : "âŒŠ"
<Multi_key> <underscore> <parenright> : "âŒ‹"
<Multi_key> <asciicircum> <parenleft> : "âŒˆ"
<Multi_key> <asciicircum> <parenright> : "âŒ‰"

# Minus-plus, to go with the standard plus-minus.
<Multi_key> <minus> <plus> : "âˆ“"

# Some equals signs.
<Multi_key> <equal> <equal> : "â‰¡"
<Multi_key> <equal> <asciitilde> : "â‰ˆ"
<Multi_key> <asciitilde> <equal> : "â‰ˆ"

# Operators.
<Multi_key> <m> <minus> : "âˆ’" # mathematical MINUS SIGN, distinct from dashes

# Tick and cross.
<Multi_key> <y> <y> : "âœ“"
<Multi_key> <n> <n> : "âœ•"

# Superscript minus, done by the same idiom as the superscript digits,
# permitting me to type sinâ»Â¹.
<Multi_key> <asciicircum> <minus> : "â»"

# Vital to be able to type ErdÅ‘s's name!
<Multi_key> <apostrophe> <apostrophe> <o> : "Å‘"
<Multi_key> <apostrophe> <apostrophe> <O> : "Å"

# Greek letters. I pondered several ways of doing this, but have
# settled on a general scheme of (Compose g) followed by the first two
# characters of the English name of the Greek letter, except for
# omicron/omega which required the first _three_ in order not to be
# ambiguous.
#
# Note that Compose m u produces U+00B5 MICRO SIGN by standard
# configuration, whereas the below configures Compose g m u to give
# U+03BC GREEK SMALL LETTER MU.

<Multi_key> <g> <a> <l>     : "Î±"
<Multi_key> <g> <b> <e>     : "Î²"
<Multi_key> <g> <g> <a>     : "Î³"
<Multi_key> <g> <d> <e>     : "Î´"
<Multi_key> <g> <e> <p>     : "Îµ"
<Multi_key> <g> <z> <e>     : "Î¶"
<Multi_key> <g> <e> <t>     : "Î·"
<Multi_key> <g> <t> <h>     : "Î¸"
<Multi_key> <g> <i> <o>     : "Î¹"
<Multi_key> <g> <k> <a>     : "Îº"
<Multi_key> <g> <l> <a>     : "Î»"
<Multi_key> <g> <m> <u>     : "Î¼"
<Multi_key> <g> <n> <u>     : "Î½"
<Multi_key> <g> <x> <i>     : "Î¾"
<Multi_key> <g> <o> <m> <i> : "Î¿"
<Multi_key> <g> <p> <i>     : "Ï€"
<Multi_key> <g> <r> <h>     : "Ï"
<Multi_key> <g> <s> <i>     : "Ïƒ"
<Multi_key> <g> <t> <a>     : "Ï„"
<Multi_key> <g> <u> <p>     : "Ï…"
<Multi_key> <g> <p> <h>     : "Ï†"
<Multi_key> <g> <c> <h>     : "Ï‡"
<Multi_key> <g> <p> <s>     : "Ïˆ"
<Multi_key> <g> <o> <m> <e> : "Ï‰"

<Multi_key> <g> <A> <l>     : "Î‘"
<Multi_key> <g> <A> <L>     : "Î‘"
<Multi_key> <g> <B> <e>     : "Î’"
<Multi_key> <g> <B> <E>     : "Î’"
